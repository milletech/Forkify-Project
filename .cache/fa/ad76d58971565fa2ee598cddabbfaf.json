umn":0},"source":"src/js/views/paginationView.js","original":{"line":79,"column":5}},{"name":"View","generated":{"line":97,"column":2},"source":"src/js/views/paginationView.js","original":{"line":5,"column":29}},{"generated":{"line":99,"column":15},"source":"src/js/views/paginationView.js","original":{"line":83,"column":15}},{"name":"PaginationView","generated":{"line":99,"column":19},"source":"src/js/views/paginationView.js","original":{"line":83,"column":19}},{"generated":{"line":99,"column":33},"source":"src/js/views/paginationView.js","original":{"line":83,"column":15}}],"sources":{"src/js/views/paginationView.js":"import icons from \"../../img/icons.svg\";\r\nimport View from \"./View.js\";\r\n\r\n\r\nclass PaginationView extends View{\r\n    _parentElement=document.querySelector(\".pagination\");\r\n    \r\n    addHandlerClick(handler){\r\n        this._parentElement.addEventListener(\"click\",function(e){\r\n            const btn=e.target.closest(\".btn--inline\");\r\n            if(!btn) return;\r\n\r\n            const goToPage=+btn.dataset.goto;\r\n\r\n            console.log(goToPage)\r\n\r\n            handler(goToPage)\r\n        })\r\n    }\r\n    _generateMarkUp(){\r\n        const numPages=Math.ceil(this._data.results.length/ this._data.resultsPerPage);\r\n        const curPage=this._data.page;\r\n        console.log(numPages)\r\n\r\n        // Page 1, and there are other Pages\r\n\r\n        if(curPage === 1 && numPages>1){\r\n            return `\r\n            <button data-goto=\"${curPage+1}\" class=\"btn--inline pagination__btn--next\">\r\n            <span>Page ${curPage+1}</span>\r\n            <svg class=\"search__icon\">\r\n              <use href=\"${icons}#icon-arrow-right\"></use>\r\n            </svg>\r\n          </button> \r\n            `\r\n        }\r\n\r\n\r\n        // Last Page\r\n\r\n        if(curPage === numPages && numPages>1){\r\n            return `\r\n            <button data-goto=\"${curPage-1}\" class=\"btn--inline pagination__btn--prev\">\r\n                <svg class=\"search__icon\">\r\n                    <use href=\"${icons}#icon-arrow-left\"></use>\r\n                </svg>\r\n                <span>Page ${curPage-1}</span>\r\n            </button>\r\n            `\r\n        }\r\n\r\n        // Other Page\r\n\r\n        if(curPage < numPages){\r\n            return `\r\n            <button data-goto=\"${curPage-1}\" class=\"btn--inline pagination__btn--prev\">\r\n                <svg class=\"search__icon\">\r\n                    <use href=\"${icons}#icon-arrow-left\"></use>\r\n                </svg>\r\n                <span>Page ${curPage-1}</span>\r\n            </button>\r\n\r\n            <button data-goto=\"${curPage+1}\" class=\"btn--inline pagination__btn--next\">\r\n                <span>Page ${curPage+1}</span>\r\n                <svg class=\"search__icon\">\r\n                <use href=\"${icons}#icon-arrow-right\"></use>\r\n                </svg>\r\n            </button> \r\n            \r\n            \r\n            \r\n            `\r\n        }\r\n\r\n        // Page 1, and there are NO other Pages\r\n\r\n        return \"Only 1 Page\"\r\n\r\n    }\r\n}\r\n\r\n\r\nexport default new PaginationView()"},"lineCount":null}},"error":null,"hash":"77f86c8d871d5f1e9a1344ea372edae2","cacheData":{"env":{}}}